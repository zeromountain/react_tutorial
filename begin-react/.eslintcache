r)\n      // }\n    case 'TOGGLE_USER':\n      return produce(state,draft => {\n        const user = draft.users.find(user => user.id === action.id);\n        user.active = !user.active;\n      });\n    \n    // return {\n      \n      //   users: state.users.map(user => \n      //     user.id === action.id\n      //       ? { ...user, active: !user.active }\n      //       : user  \n      //   )\n      // }\n    case 'REMOVE_USER':\n      return produce(state, draft => {\n        const index = draft.users.findIndex(user => user.id ===   action.id);\n        draft.users.splice(index, 1);\n      })\n      \n      // return {\n        \n      //   users: state.users.filter(user => user.id !== action.id)\n      // }\n    default:\n      throw new Error('Un  handled action');\n  }\n}\n\nexport const UserDispatch = createContext(null);\n\nfunction App() {\n\n  const [state, dispatch] = useReducer(reducer, initialState);\n\n  const { users } = state;\n\n  const count = useMemo(() => countActiveUsers(users, [users]));\n\n\n  return (\n    <UserDispatch.Provider value={dispatch}>\n      <CreateUser />\n      <UserList users={users} />\n      <div>활성 사용자 수: {count} </div>\n    </UserDispatch.Provider>\n  );\n}\n\nexport default App;\n","/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/Hello.js",["47","48","49"],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/reportWebVitals.js",[],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/components/Counter.js",["50","51"],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/components/InputSample.js",[],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/index.js",["52"],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/components/UserList.js",["53"],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/components/useInputs.js",["54"],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/compon