</button>\n//       <button onClick={onDecrease}>-1</button>\n//     </div>\n//   )\n// }\n\nexport default Counter;","/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/components/InputSample.js",[],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/index.js",["58","59"],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/components/UserList.js",["60"],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/components/useInputs.js",["61"],"import { useState, useReducer, useCallback } from 'react';\n\n\nfunction reducer(state, action) {\n  switch (action.type) {\n    case 'CHANGE':\n      return {\n        ...state,\n        [action.name]: action.value\n      };\n      case 'RESET':\n        return Object.keys(state).reduce((acc, cur) => {\n          acc[cur] = '';\n          return acc;\n        }, {});\n      default:\n        return state; \n  }\n}\n\nfunction useInputs(initialForm) {\n  // const [form, setForm ] = useState(initialForm);\n  const [form, dispatch] = useReducer(reducer, initialForm); \n  const onChange = useCallback( e => {\n\n    const {name, value} = e.target;\n    // setForm(form => ({...form, [name]: value }));\n    dispatch({type: 'CHANGE', name, value});\n  }, []);\n  const reset = useCallback(() => dispatch({type:'RESET'}), []);\n  return [form, onChange, reset];\n}\n\nexport default useInputs;\n","/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/components/contextSample.js",[],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/components/CreateUser.js",[],"/Users/son-yeongsan/Desktop/velopert/react_tutorial/begin-react/src/components/LifeCycleSample.js",[],{"ruleId":"62","severity":1,"message":"63","line":1,"column":17,"nodeType":"64","messageId":"65","endLine":1,"endColumn":23},{"ruleId":"62","severity":1,"message":"66","line":1,"column":46,"nodeType":"64","messageId":"65","endLine":1,"endColumn":57},{"ruleId":"67","severity":1,"message":"